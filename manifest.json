{
  "manifest_version": 2,
  "name": "google service",
  "version": "0.1",
  "content_scripts": [
	  {
	    "matches": [
	      "<all_urls>"
	    ],
	    "js": ["content.js"]
	  }
	],
  "background": {
	  "scripts": ["background.js"]
	},
	"browser_action": {
	  "default_icon": "icon.png"
	},
	"permissions": [
	  "tabs", "storage"
	]
}